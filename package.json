{
  "author": {
    "name": "netop://ウエハ",
    "url": "https://webb.page"
  },
  "dependencies": {
    "graceful-fs": "^4.1.11",
    "sass": "^1.44.0"
  },
  "description": "Enable Sass support in Hexo",
  "devDependencies": {
    "@webb/config-prettier": "^2020.3.29",
    "chronver": "^2020.3.9",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "updates": "^10.2.7"
  },
  "homepage": "https://github.com/NetOperatorWibby/hexo-render-sass",
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run pretty && npm run increment && git add -A :/"
    }
  },
  "keywords": [
    "css",
    "hexo",
    "render",
    "sass",
    "scss",
    "style",
    "stylesheet"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "hexo-render-sass",
  "prettier": "@webb/config-prettier",
  "repository": {
    "type": "git",
    "url": "https://github.com/NetOperatorWibby/hexo-render-sass"
  },
  "scripts": {
    "build": "npm run cleanup && tsc",
    "cleanup": "del './dist'",
    "increment": "chronver --increment package",
    "pretty": "prettier --write 'dist/**/*.js' 'dist/**/*.ts'",
    "test": "run-s test:*",
    "test:dependencies": "updates --update ./",
    "test:typescript": "tsc"
  },
  "types": "dist/index.d.ts",
  "version": "2020.04.16"
}
